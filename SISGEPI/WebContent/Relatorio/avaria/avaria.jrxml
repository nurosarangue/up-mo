	<parameter name="idCliente" class="java.lang.Long"/>
	<queryString language="SQL">
		<![CDATA[SELECT sisgepi.tipodeavaria.descricao AS tipoavaria,
	sisgepi.avaria.detalhes,
	sisgepi.avaria.`dataCadastro`,
	sisgepi.cidade.nome AS cidade,
	sisgepi.cliente.nome AS nomeCliente,
	sisgepi.cliente.instalacao,
	sisgepi.cliente.telefone,
	sisgepi.cliente.avenida,
	sisgepi.cliente.contador,
	sisgepi.cliente.bairo
FROM sisgepi.avaria
	INNER JOIN sisgepi.cliente ON 
	 sisgepi.avaria.id_cliente = sisgepi.cliente.id 
	INNER JOIN sisgepi.cidade ON 
	 sisgepi.cliente.id_cidade = sisgepi.cidade.id 
	INNER JOIN sisgepi.tipodeavaria ON 
	 sisgepi.avaria.`id_tipoAvaria` = sisgepi.tipodeavaria.id 
WHERE 
	 sisgepi.cliente.id = $P{idCliente}]]>
	</queryString>
